#include "main.h"

/**
 * *_strcat - joins two strings
 * @dest: destination string
 * @src: string to be appended to dest
 * Return: joined string
 */

char *_strcat(char *dest, char *src)
{
int i = 0, j = 0;

char *join = de;
while (src[i] != '\0')
{
  dest++ = src[i];
  i++;
}


return (dest);
}
